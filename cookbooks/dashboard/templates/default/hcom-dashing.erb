# Starts and stops rpc.mountd
#


case "$1" in
start)

	echo "start"	
;;

stop)
	
	echo "stop"	
;;

restart)
  	$0 stop
  	$0 start
;;

status)
             if pidof -o %PPID rpc.mountd > /dev/null; then
                     echo "Running"
                     exit 0
             else
                     echo "Not running"
                     exit 1
             fi
;;

*)
        echo "Usage: $0 {start|stop|restart|status}"
        exit 1
esac

